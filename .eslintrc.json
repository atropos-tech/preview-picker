{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react", "import"
    ],
    "rules": {
        "semi": [
            "error",
            "always"
        ],
        "import/no-unresolved": "error",
        "import/named": "error",
        "import/default": "error"
    }
}